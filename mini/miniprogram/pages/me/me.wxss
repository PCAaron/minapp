/* miniprogram/pages/me/me.wxss */
page{
    background-color: #f1f1f1;
}
.profile-header{
    background-image: url('data:image/jpeg;base64,');
    background-size: cover;
    height: 380rpx;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    color:#fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0,0,0,.3);
}

.avatar-url{
    width: 200rpx;
    height: 200rpx;
    display: block;
    overflow: hidden;
    border: 6rpx solid #fff;
    border-radius: 50%;
}

.nav{
    margin: 60rpx 30rpx;
    background-color: #fff;
}
.nav-item{
    display: flex;
    align-items: center;
    flex-direction: row;
    border-bottom: 1px solid #eee;
    position: relative;
}
.content{
    width: 100%;
    height: 100%;
    padding: 30rpx 20rpx;
}
.icon-right{
    color:#FF59D3;
}
.text{
    padding: 0 20rpx;
    flex: 1;
}
.icon-xiangyou{
    position: absolute;
    right: 20rpx;
    top: 36rpx;
}
