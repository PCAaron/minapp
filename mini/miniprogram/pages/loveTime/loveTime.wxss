.profile-header{
    height: 380rpx;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    color:#fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0,0,0,.3);
}

.avatar-url{
    width: 200rpx;
    height: 200rpx;
    display: block;
    overflow: hidden;
    border: 6rpx solid #fff;
    border-radius: 50%;
}
.weui-cell-third{
    background: #fff;
}
.weui-cell-list{
    background: #fff;
    margin: 10rpx 20rpx 10rpx 100rpx;
}
.weui-cell-line{
   /* width: 100px; */
    margin-left: 10rpx;
    border-left: 2rpx solid #ddd;
    height: 350rpx;
    background: #fff
}
.weui-cell-circle{
    border-radius: 20rpx;
    width: 20rpx;
    height: 20rpx;
    background-color: #FFA5C2;
}
.weui-cell-time{
    /* width: 50px; */
    float: left;
    font-size:14px;
    padding-left: 30rpx;
    width: 144rpx;
}
.weui-cell-event{
    padding-top: 30rpx;
    padding-left: 30rpx;
}
.weui-cell-name{
   font-size:14px;
   height:46rpx;
   margin-left: 200rpx;
}